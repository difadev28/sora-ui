{
  "name": "@sore-ui-material/spinner",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "style": "./src/Spinner.module.scss",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "prebuild": "powershell -Command \"if (!(Test-Path ./src/assets)) { New-Item -ItemType Directory -Force -Path ./src/assets }\" && npx @tailwindcss/cli -i ../../../../src/styles/index.css -o ./src/assets/output.css",
    "build": "vite build"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
